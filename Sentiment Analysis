         \"had\", \",\", \"!\", \"'t\", \"n\\'t\", \")\", \"ordered\", \"which\", \"'s\", \"(\", \"as\", \"me\", \"their\", \n            \"also\", \"can\", \"or\", \"do\", \"just\", \"be\", \"would\", \"did\", \"``\", \"really\", \"try\", \"here\", \"if\", \n            \"come\", \"get\", \"will\"]\n\n# Preprocess text data\ndef preprocess_text(text, stopwords):\n    # Tokenize the text\n    words = word_tokenize(text)\n    # Remove stopwords\n    filtered_words = [word for word in words if word.lower() not in stopwords]\n    # Join the filtered words back into a single string\n    processed_text = ' '.join(filtered_words)\n    return processed_text\n\n# Assuming 'df' is your DataFrame containing the preprocessed text data\ndocuments = df[0].tolist()\n\n# Apply the preprocess_text function to each document\ndocuments = [preprocess_text(doc, stopwords) for doc in documents]\n\n# Tokenize the documents\ntokenized_docs = [doc.split() for doc in documents]\n\n# Create a dictionary representation of the documents\ndictionary = corpora.Dictionary(tokenized_docs)\n\n# Filter out tokens that appear in less than 20 documents or more than 50% of the documents\ndictionary.filter_extremes(no_below=20, no_above=0.5)\n\n# Convert the dictionary to a bag-of-words representation\ncorpus = [dictionary.doc2bow(doc) for doc in tokenized_docs]\n\n# Train the LDA model\nlda_model = LdaModel(corpus, num_topics=5, id2word=dictionary, passes=20)\n\n# Initialize a set to store the topics already printed\nprinted_topics = set()\n\n# Get the topics generated by the model\ntopics = lda_model.print_topics(num_words=3)\n\n# Print the topics\nfor topic in topics:\n    topic_words = topic[1].split('\"')[1::2]  # Extracting the words from the topic string\n    if topic_words[0] not in printed_topics:  # Check if the first word of the topic has already been printed\n        print(topic)\n        printed_topics.add(topic_words[0])  # Add the first word to the set of printed topics\n\n# Assign topics to documents\nlda_corpus = [lda_model[doc] for doc in corpus]\n\n# Add topic assignments to DataFrame\ndf['topic'] = [max(lda_doc, key=lambda x: x[1])[0] for lda_doc in lda_corpus]\n\n# Analyze topic distribution in positive and negative reviews\npositive_reviews = df[df['predicted_sentiment'] == 'positive']\nnegative_reviews = df[df['predicted_sentiment'] == 'negative']\nneutral_reviews = df[df['predicted_sentiment'] == 'neutral']\n\npositive_topic_distribution = positive_reviews['topic'].value_counts(normalize=True)\nnegative_topic_distribution = negative_reviews['topic'].value_counts(normalize=True)\nneutral_topic_distribution = neutral_reviews['topic'].value_counts(normalize=True)\n\nprint(\"\\nPositive Topic Distribution:\")\nprint(positive_topic_distribution)\nprint(\"\\nNegative Topic Distribution:\")\nprint(negative_topic_distribution)\nprint(\"\\nNeutral Topic Distribution:\")\nprint(neutral_topic_distribution)\n","metadata":{"execution":{"iopub.status.busy":"2024-04-19T16:35:06.171313Z","iopub.execute_input":"2024-04-19T16:35:06.172027Z","iopub.status.idle":"2024-04-19T16:35:07.573273Z","shell.execute_reply.started":"2024-04-19T16:35:06.171944Z","shell.execute_reply":"2024-04-19T16:35:07.571916Z"},"trusted":true},"execution_count":55,"outputs":[{"name":"stdout","text":"(0, '0.342*\"noodles\" + 0.148*\"sushi\" + 0.058*\"poke\"')\n(1, '0.266*\"food\" + 0.172*\"good\" + 0.079*\"sushi\"')\n(2, '0.199*\"soup\" + 0.154*\"noodle\" + 0.109*\"good\"')\n(3, '0.373*\"tea\" + 0.194*\"bubble\" + 0.073*\"place\"')\n(4, '0.197*\"time\" + 0.174*\"order\" + 0.162*\"like\"')\n\nPositive Topic Distribution:\ntopic\n1    0.250000\n2    0.224138\n3    0.198276\n4    0.172414\n0    0.155172\nName: proportion, dtype: float64\n\nNegative Topic Distribution:\ntopic\n3    0.357143\n1    0.285714\n2    0.142857\n4    0.142857\n0    0.071429\nName: proportion, dtype: float64\n\nNeutral Topic Distribution:\ntopic\n4    1.0\nName: proportion, dtype: float64\n","output_type":"stream"}]