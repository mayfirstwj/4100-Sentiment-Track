\ndf['predicted_sentiment'] = df['sentiment_score'].apply(categorize_sentiment)\n\n# Plot the sentiment distribution\nplt.figure(figsize=(8, 6))\nax = sentiment_counts.plot(kind='bar', color='lightgreen')\nplt.title('Sentiment Distribution')\nplt.xlabel('Sentiment Label')\nplt.ylabel('Count')\nplt.xticks(rotation=0)\nplt.grid(axis='y', linestyle='--', alpha=0.7)\n\n# Add text annotations to display the count of reviews for each sentiment label\nfor i, count in enumerate(sentiment_counts):\n    ax.text(i, count, str(count), ha='center', va='bottom')\n\n# Calculate percentage of each sentiment\nsentiment_percentage = (sentiment_counts / len(df)) * 100\n\n# Print the percentage of each sentiment\nprint(\"Sentiment Distribution:\")\nprint(\"Positive: {:.2f}%\".format(sentiment_percentage['positive']))\nprint(\"Negative: {:.2f}%\".format(sentiment_percentage['negative']))\nprint(\"Neutral: {:.2f}%\".format(sentiment_percentage['neutral']))\n\n","metadata":{"execution":{"iopub.status.busy":"2024-04-19T22:31:12.740344Z","iopub.execute_input":"2024-04-19T22:31:12.740751Z","iopub.status.idle":"2024-04-19T22:31:13.285184Z","shell.execute_reply.started":"2024-04-19T22:31:12.740721Z","shell.execute_reply":"2024-04-19T22:31:13.283915Z"},"trusted":true},"execution_count":4,"outputs":[{"name":"stdout","text":"Sentiment Distribution:\nPositive: 88.55%\nNegative: 10.69%\nNeutral: 0.76%\n","output_type":"stream"},{"output_type":"display_data","data":{"text/plain":"<Figure size 800x600 with 1 Axes>","image/png":""},"metadata":{}}]},{"cell_type":"code","source":"# List all positive reviews\npositive_reviews = df[df['predicted_sentiment'] == 'positive'][0].tolist()\nprint(\"Positive Reviews:\")\nfor review in positive_reviews:\n    print(review)\n    print()\n\n# List all negative reviews\nnegative_reviews = df[df['predicted_sentiment'] == 'negative'][0].tolist()\nprint(\"Negative Reviews:\")\nfor review in negative_reviews:\n    print(review)\n    print()\n\n# List all neutral reviews\